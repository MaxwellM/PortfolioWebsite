doctype html
html
    meta(name='description', content="Maxwell Morin's portfolio website")
    meta(name='keywords', content="HTML,CSS,JavaScript,AngularJS,Go,PostgreSQL")
    meta(name='author', content="Maxwell Morin")
    //meta(name='viewport' content='width=device-width, "initial-scale=1.0')
    head
        link(rel="stylesheet", href="./css/main.css", type="text/css")
        link(rel="stylesheet", href="./css/projects.css", type="text/css")
        link(rel="stylesheet", href="./css/footer.css", type="text/css")
        link(rel="stylesheet", href="./css/navigation.css", type="text/css")
        link(rel="stylesheet", href="./css/contactMe.css", type="text/css")
        link(rel="stylesheet", href="./css/resume.css", type="text/css")
        link(rel="stylesheet", href="./css/angularJSExampleTable.css", type="text/css")
        link(rel="stylesheet", href="./css/angularJSExampleDisplay.css", type="text/css")
        link(rel="stylesheet", href="./css/angularJSExampleChart.css", type="text/css")
        link(rel="stylesheet", href="./css/unityLostInSpace.css", type="text/css")
        link(rel="stylesheet", href="./css/phraseOccurrence.css", type="text/css")
        link(rel="stylesheet", href="./css/translate.css", type="text/css")
        link(rel="stylesheet", href="./unityGames/LostInSpaceWebGL/TemplateData/style.css")
        link(rel="stylesheet", href="./css/twitter.css", type="text/css")
        link(rel="stylesheet", href="./js/node_modules/angular-tooltips/dist/angular-tooltips.css", type="text/css")
        link(rel="stylesheet", href="./js/node_modules/github-calendar-1.3.4/dist/github-calendar.css", type="text/css")
        link(rel="stylesheet", href="./js/node_modules/angular-material/angular-material.css", type="text/css")
        link(rel="stylesheet", href="./js/node_modules/angular-material-data-table/dist/md-data-table.css", type="text/css")
        link(rel="stylesheet", href="./css/c3.css")
        link(rel="shortcut icon", href="./images/favicon.png", type="image/png")
        script(src="./js/node_modules/angular/angular.js")
    body(ng-app='app', ng-controller='myCtrl')
        div#MainWrapper
            div#nav_bar(ng-cloak)
                include includes/navigation
            div#mainPage(ng-controller='mainPageCtrl', ng-show='myCtrl.buttonName == "Maxwell Morin"')
                include includes/main
            div#projectPage(ng-controller='projectCtrl', ng-cloak, ng-show='myCtrl.buttonName == "Projects"')
                include includes/projectPage
            div#resumePage(ng-controller='resumeCtrl', ng-cloak , ng-show='myCtrl.buttonName == "Resume"')
                include includes/resume
            div#contactMePage(ng-controller='contactMeCtrl', ng-cloak, ng-show='myCtrl.buttonName == "Contact Me"')
                include includes/contactMePage
            div#angularJSExampleTablePage(ng-controller='angularJSExampleTableCtrl', ng-cloak, ng-show='myCtrl.buttonName == "Table Example"')
                include includes/angularJSExampleTable
            div#angularJSExampleDisplayPage(ng-controller='angularJSExampleDisplayCtrl', ng-cloak, ng-show='myCtrl.buttonName == "Display Example"')
                include includes/angularJSExampleDisplay
            div#angularJSExampleChartPage(ng-controller='angularJSExampleChartCtrl', ng-cloak, ng-show='myCtrl.buttonName == "Chart Example"')
                include includes/angularJSExampleChart
            div#untyGameLostInSpacePage(ng-cloak, ng-show='myCtrl.buttonName == "Unity Lost In Space"')
                include includes/unityLostInSpace
            div#goExampleOccurrencePage(ng-controller='goExampleOccurrenceCtl', ng-cloak, ng-show='myCtrl.buttonName == "String Duplication"')
                include includes/phraseOccurrence
            div#goExampleTranslatePage(ng-controller='goExampleTranslateCtl', ng-cloak, ng-show='myCtrl.buttonName == "Translate"')
                include includes/translate
            div#goExampleTwitterPage(ng-controller='goExampleTwitterCtl', ng-cloak, ng-show='myCtrl.buttonName == "Twitter"')
                include includes/twitter
            div#footerWrapper(ng-controller='footerCtrl', ng-cloak)
                include includes/footer
    script(src="./js/node_modules/angular-sanitize/angular-sanitize.js")
    script(src="./js/node_modules/angular-tooltips/dist/angular-tooltips.min.js")
    script(src="./js/node_modules/github-calendar-1.3.4/dist/github-calendar.min.js")
    script(src="./js/node_modules/angular-material/angular-material.js")
    script(src="./js/node_modules/angular-material-data-table/dist/md-data-table.js")
    script(src="./js/node_modules/angular-animate/angular-animate.js")
    script(src="./js/node_modules/angular-aria/angular-aria.js")
    script(src="./js/node_modules/angular-messages/angular-messages.js")
    script(src="./js/node_modules/ngtweet/dist/ngtweet.js")
