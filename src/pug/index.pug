doctype html
html
    head
        meta(name='description', content="Maxwell Morin's portfolio website")
        meta(name='keywords', content="HTML,CSS,JavaScript,AngularJS,Go,PostgreSQL")
        meta(name='author', content="Maxwell Morin")
        meta(name='title' property='og:title' content='[Maxwell Morins Portfolio Website]')
        //meta(property='og:title' content="Maxwell Morin's Portfolio Website")
        meta(name='image' property='og:image' content='[https://www.maxinotsh.org/images/me5.jpg]')
        //meta(property='og:image' content='/images/me5.jpg')
        meta(name='description' property='og:description' content='[Personal portfolio website entirely built and managed by myself that demonstrates the capability of full-stack development by showcasing unique array of sought after practices]')
        //meta(property='og:description' content='Personal portfolio website entirely built and managed by myself that demonstrates the capability of full-stack development by showcasing unique array of sought after practices')
        meta(name='url' property='og:url' content='//www.maxintosh.org/')
        link(rel="stylesheet", href="./js/node_modules/angular-tooltips/dist/angular-tooltips.css", type="text/css")
        link(rel="stylesheet", href="./js/node_modules/angular-material/angular-material.css", type="text/css")
        link(rel="stylesheet", href="./js/node_modules/angular-material-data-table/dist/md-data-table.css", type="text/css")
        link(rel="stylesheet", href="./css/main.css", type="text/css")
        link(rel="shortcut icon", href="./images/favicon.png", type="image/png")
        script(defer='', src="./js/node_modules/angular/angular.min.js")
        script(defer='', src="./js/app.js")
    body(ng-app='app', ng-controller='myCtrl')
        div#MainWrapper
            div#nav_bar(ng-controller='navigationCtrl', ng-cloak)
                include includes/navigation
            div#mainPage(ng-controller='mainPageCtrl', ng-if='buttonName == "Maxwell Morin"')
                include includes/main
            div#projectPage(ng-controller='projectCtrl', ng-cloak, ng-if='buttonName == "Projects"')
                include includes/projectPage
            div#resumePage(ng-controller='resumeCtrl', ng-cloak , ng-if='buttonName == "Resume"')
                include includes/resume
            div#contactMePage(ng-controller='contactMeCtrl', ng-cloak, ng-if='buttonName == "Contact Me"')
                include includes/contactMePage
            div#angularJSExampleTablePage(ng-controller='angularJSExampleTableCtrl', ng-cloak, ng-if='buttonName == "Table Example"')
                include includes/angularJSExampleTable
            div#angularJSExampleDisplayPage(ng-controller='angularJSExampleDisplayCtrl', ng-cloak, ng-if='buttonName == "Display Example"')
                include includes/angularJSExampleDisplay
            div#angularJSExampleChartPage(ng-controller='angularJSExampleChartCtrl', ng-cloak, ng-if='buttonName == "Chart Example"')
                include includes/angularJSExampleChart
            div#untyGameEggmanPage(ng-controller='unityEggmanCtrl', ng-cloak, ng-if='buttonName == "Unity Eggman"')
                include includes/unityEggman
            div#untyGameEggciteBikePage(ng-controller='unityEggciteBikeCtrl', ng-cloak, ng-if='buttonName == "Unity Eggcite Bike"')
                include includes/unityEggciteBike
            div#untyGameLostInSpacePage(ng-controller='unityLostInSpaceCtrl', ng-cloak, ng-if='buttonName == "Unity Lost In Space"')
                include includes/unityLostInSpace
            div#goExampleOccurrencePage(ng-controller='goExampleOccurrenceCtl', ng-cloak, ng-if='buttonName == "String Duplication"')
                include includes/phraseOccurrence
            div#goExampleTranslatePage(ng-controller='goExampleTranslateCtl', ng-cloak, ng-if='buttonName == "Translate"')
                include includes/translate
            div#goExampleTwitterPage(ng-controller='goExampleTwitterCtl', ng-cloak, ng-if='buttonName == "Twitter"')
                include includes/twitter
            div#stockTrackerPage(ng-controller='stockTrackerCtrl', ng-cloak, ng-if='buttonName == "Stock Tracker"')
                include includes/stockTracker
            div#footerWrapper(ng-controller='footerCtrl', ng-cloak)
                include includes/footer
    script(defer='', src="./js/node_modules/angular-material/angular-material.min.js")
    script(defer='', src="./js/node_modules/angular-material-data-table/dist/md-data-table.min.js")
    script(defer='', src="./js/node_modules/angular-sanitize/angular-sanitize.min.js")
    script(defer='', src="./js/node_modules/angular-tooltips/dist/angular-tooltips.min.js")
    script(defer='', src="./js/node_modules/angular-animate/angular-animate.min.js")
    script(defer='', src="./js/node_modules/angular-aria/angular-aria.min.js")
    script(defer='', src="./js/node_modules/angular-messages/angular-messages.min.js")
    script(defer='', src="./js/node_modules/angular-cookies/angular-cookies.min.js")
    // Global site tag (gtag.js) - Google Analytics
    script(async='', src='https://www.googletagmanager.com/gtag/js?id=UA-19197796-1')
    script.
        window.dataLayer = window.dataLayer || [];
        function gtag() {
            dataLayer.push(arguments);
        }
        gtag('js', new Date());
        gtag('config', 'UA-19197796-1');
