title AngularJS and C3JS Chart Example!
meta(name='description', content="AngulaJS, C3, and JavaScript example")
meta(name='keywords', content="HTML,CSS,JavaScript,AngularJS,C3,Go,PostgreSQL")
meta(name='author', content="Maxwell Morin")
meta(name='viewport' content='width=device-width, initial-scale=1.0')
div#angularJSExampleChartInnerPageWrapper
    div#angularJSExampleChartTopWrapper
        div#angularJSExampleChartTitle
            h1 Unique Visitors by Month
        div#angularJSExampleChartWrapper
            div#chart
    div#angularJSExampleChartBottomWrapper
        div#angularJSExampleChartVisitorCount
            h3 Unique Visitors for {{ ::currentMonthName }}
            h3 {{ ::currentMonthTotal }}
        div#angularJSExampleChartVisitorList
            h3 Unique Visitor List
            label(ng-repeat='ip in ipLocationList', ng-click='selectIPLocation(ip.ip)') {{ ::$index + 1 }}) {{ ::ip.ip }}
        div#angularJSExampleChartVisitorLocation(ng-show='selectedIP.length > 0')
            h3 Unique Visitor Locations
            div#angularJSExampleChartVisitors(ng-repeat='visitor in selectedIP')
                div#angularJSExampleChartLocationLabelWrapper
                    label(ng-show='visitor.ip') IP
                    label(ng-show='visitor.isp') ISP
                    label(ng-show='visitor.as.name') Name
                    label(ng-show='visitor.location.city') City
                    label(ng-show='visitor.location.region') Region
                    label(ng-show='visitor.location.county') Country
                div#angularJSExampleChartLocationInfoWrapper
                    label(ng-show='visitor.ip') {{ ::visitor.ip }}
                    label(ng-show='visitor.isp') {{ ::visitor.isp }}
                    label(ng-show='visitor.as.name') {{ ::visitor.as.name }}
                    label(ng-show='visitor.location.city') {{ ::visitor.location.city }}
                    label(ng-show='visitor.location.region') {{ ::visitor.location.region }}
                    label(ng-show='visitor.location.county') {{ ::visitor.location.county }}


